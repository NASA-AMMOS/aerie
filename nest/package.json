{
  "name": "nest",
  "version": "2.2.0",
  "private": true,
  "scripts": {
    "build-prod": "ng build --configuration=prod",
    "build-prod-mpsserver": "ng build --outputPath=dist-mpsserver --configuration=prod-mpsserver --base-href /mpsserver/raven/ --deploy-url /mpsserver/raven/",
    "docs": "compodoc -p src/tsconfig.app.json -s",
    "e2e": "protractor e2e/protractor.conf.js ",
    "integration-test": "ng serve --disable-host-check & bash ../robot/run.sh",
    "license:check": "/bin/bash ../scripts/license-headers -d -l ../scripts/license-template.txt -e js -s \"Copyright\" ./src/app/ && /bin/bash ../scripts/license-headers -d -l ../scripts/license-template.txt -e ts -s \"Copyright\" ./src/app/ && /bin/bash ../scripts/license-headers -d -l ../scripts/license-template.txt -e css -s \"Copyright\" ./src/app/",
    "license:write": "/bin/bash ../scripts/license-headers -l ../scripts/license-template.txt -e js -s \"Copyright\" ./src/app/ && /bin/bash ../scripts/license-headers -l ../scripts/license-template.txt -e ts -s \"Copyright\" ./src/app/ && /bin/bash ../scripts/license-headers -l ../scripts/license-template.txt -e css -s \"Copyright\" ./src/app/",
    "lint": "ng lint",
    "lint-prod": "ng lint --tslintConfig=./src/tslint.prod.json",
    "ng": "ng",
    "prebuild-prod": "npm run version",
    "prebuild-prod-mpsserver": "npm run version",
    "prestart": "npm run version",
    "pretest-for-build": "npm run version",
    "start": "ng serve",
    "start-dev": "bash ../scripts/post-adaptation.sh && ng serve",
    "test": "ng test --code-coverage --source-map=false",
    "test-for-build": "ng test --code-coverage --watch=false --browsers FirefoxHeadless",
    "version": "ts-node git.version.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --no-restage --pattern 'src/**/*.{js,ts,css,html} --verbose --bail' && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "node ./scripts/lint-staged.js lint-prod",
      "git add"
    ]
  },
  "dependencies": {
    "@angular/animations": "^7.2.14",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^7.2.14",
    "@angular/compiler": "^7.2.14",
    "@angular/core": "^7.2.14",
    "@angular/flex-layout": "7.0.0-beta.19",
    "@angular/forms": "^7.2.14",
    "@angular/http": "^7.2.14",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "^7.2.14",
    "@angular/platform-browser-dynamic": "^7.2.14",
    "@angular/router": "^7.2.14",
    "@ngrx/effects": "~7.2.0",
    "@ngrx/router-store": "~7.2.0",
    "@ngrx/store": "~7.2.0",
    "@ngrx/store-devtools": "~7.2.0",
    "@types/moment-timezone": "^0.5.12",
    "ag-grid-angular": "^19.1.2",
    "ag-grid-community": "^19.1.4",
    "angular-sortablejs": "^2.7.0",
    "angular-split": "^2.0.1",
    "codemirror": "^5.46.0",
    "core-js": "^2.6.5",
    "d3": "^5.9.2",
    "file-saver": "^1.3.8",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.11",
    "material-design-icons": "^3.0.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.25",
    "ng-pick-datetime": "^7.0.0",
    "ngx-color": "^2.0.6",
    "ngx-toastr": "^10.0.2",
    "rxjs": "~6.3.3",
    "rxjs-compat": "~6.3.3",
    "sortablejs": "^1.9.0",
    "strip-json-comments": "^2.0.1",
    "tslib": "^1.9.0",
    "uuid": "^3.3.2",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular/cli": "^7.3.8",
    "@angular/compiler-cli": "^7.2.14",
    "@angular/language-service": "^7.2.14",
    "@compodoc/compodoc": "^1.1.9",
    "@ngrx/schematics": "^7.4.0",
    "@types/chai": "^4.1.7",
    "@types/codemirror": "0.0.70",
    "@types/d3": "^5.7.2",
    "@types/file-saver": "^1.3.1",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "^2.0.6",
    "@types/lodash": "^4.14.123",
    "@types/node": "~8.9.4",
    "@types/strip-json-comments": "0.0.30",
    "@types/uuid": "^3.4.4",
    "chai": "^4.2.0",
    "codelyzer": "~4.5.0",
    "husky": "^1.3.1",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "^0.4.1",
    "jasmine-spec-reporter": "^4.2.1",
    "jasminewd": "^1.1.0",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "lint-staged": "^8.1.5",
    "ngrx-store-freeze": "^0.2.4",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0",
    "protractor": "^5.4.2",
    "sonar-scanner": "^3.1.0",
    "sonarjs": "^1.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-jasmine": "^1.0.3",
    "typescript": "~3.2.2"
  }
}
