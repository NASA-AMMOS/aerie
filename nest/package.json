{
  "name": "nest",
  "version": "2.2.0",
  "private": true,
  "scripts": {
    "build-prod": "ng build --configuration=prod",
    "build-prod-mpsserver": "ng build --outputPath=dist-mpsserver --configuration=prod-mpsserver --base-href /mpsserver/raven/ --deploy-url /mpsserver/raven/",
    "docs": "compodoc -p src/tsconfig.app.json -s",
    "e2e": "ng e2e --webdriverUpdate=true",
    "format:check": "prettier --config ./.prettierrc.json --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "format:write": "prettier --config ./.prettierrc.json --write \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "license:check": "/bin/bash ../scripts/license-headers -d -l ../scripts/license-template.txt -e js -s \"Copyright\" ./src/app/ && /bin/bash ../scripts/license-headers -d -l ../scripts/license-template.txt -e ts -s \"Copyright\" ./src/app/ && /bin/bash ../scripts/license-headers -d -l ../scripts/license-template.txt -e css -s \"Copyright\" ./src/app/",
    "license:write": "/bin/bash ../scripts/license-headers -l ../scripts/license-template.txt -e js -s \"Copyright\" ./src/app/ && /bin/bash ../scripts/license-headers -l ../scripts/license-template.txt -e ts -s \"Copyright\" ./src/app/ && /bin/bash ../scripts/license-headers -l ../scripts/license-template.txt -e css -s \"Copyright\" ./src/app/",
    "lint": "ng lint",
    "ng": "ng",
    "prebuild-prod": "npm run version",
    "prebuild-prod-mpsserver": "npm run version",
    "prestart": "npm run version",
    "pretest-for-build": "npm run version",
    "protractor": "protractor ./e2e/protractor.conf.js",
    "start": "ng serve",
    "test": "ng test --code-coverage --source-map=false",
    "test-for-build": "ng test --code-coverage --watch=false --browsers FirefoxHeadless",
    "version": "ts-node git.version.ts"
  },
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "~7.3.3",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/flex-layout": "7.0.0-beta.19",
    "@angular/forms": "~7.2.0",
    "@angular/http": "~7.2.0",
    "@angular/material": "~7.3.3",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@ngrx/effects": "~7.2.0",
    "@ngrx/router-store": "~7.2.0",
    "@ngrx/store": "~7.2.0",
    "@ngrx/store-devtools": "~7.2.0",
    "@types/moment-timezone": "^0.5.10",
    "ag-grid-angular": "^19.0.0",
    "ag-grid-community": "^19.0.0",
    "angular-sortablejs": "^2.6.0",
    "angular-split": "^2.0.1",
    "codemirror": "^5.42.0",
    "core-js": "^2.5.7",
    "d3": "^5.7.0",
    "file-saver": "^1.3.8",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.11",
    "material-design-icons": "^3.0.1",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.23",
    "ngx-toastr": "^9.1.1",
    "roboto-fontface": "^0.10.0",
    "rxjs": "~6.3.3",
    "rxjs-compat": "~6.3.3",
    "sortablejs": "^1.7.0",
    "sonar-scanner": "^3.1.0",
    "sonarjs": "^1.0.0",
    "strip-json-comments": "^2.0.1",
    "tslib": "^1.9.0",
    "uuid": "^3.3.2",
    "web-animations-js": "^2.3.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.2",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@compodoc/compodoc": "^1.1.8",
    "@ngrx/schematics": "^7.2.0",
    "@types/chai": "^4.1.6",
    "@types/codemirror": "0.0.70",
    "@types/cucumber": "^4.0.4",
    "@types/d3": "^5.0.0",
    "@types/file-saver": "^1.3.0",
    "@types/jasmine": "~2.8.8",
    "@types/lodash": "^4.14.116",
    "@types/node": "~8.9.4",
    "@types/strip-json-comments": "0.0.30",
    "@types/uuid": "^3.4.4",
    "chai": "^4.2.0",
    "codelyzer": "~4.5.0",
    "cucumber": "^5.1.0",
    "cucumber-pretty": "^1.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "^0.4.0",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "ngrx-store-freeze": "^0.2.4",
    "prettier": "^1.14.3",
    "protractor": "~5.4.0",
    "protractor-cucumber-framework": "^6.1.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "~3.2.2"
  }
}
