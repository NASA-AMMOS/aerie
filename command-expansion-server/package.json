{
  "name": "aerie-command",
  "version": "1.0.0",
  "description": "command service for AERIE",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "rm -rf ./build/packages/lib/CommandEDSLPreface.ts; tsc; cp ./src/packages/lib/CommandEDSLPreface.ts ./build/packages/lib/CommandEDSLPreface.ts;",
    "start": "node ./build/app.js",
    "ts-watch": "tsc --watch",
    "dev-server": "supervisor -- --inspect=0.0.0.0 ./build/app.js",
    "dev": "concurrently -n \"tsc,node\" -c \"blue,red\" \"npm run ts-watch\" \"npm run dev-server\"",
    "debug": "node --inspect=0.0.0.0:9229 ./build/app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@microsoft/tsdoc": "^0.13.2",
    "@nasa-jpl/aerie-ampcs": "^1.0.0",
    "body-parser": "^1.19.2",
    "express": "^4.17.2",
    "graphql": "^16.3.0",
    "pg": "^8.7.3",
    "reserved-words": "^0.1.2",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/pg": "^8.6.4",
    "@types/reserved-words": "^0.1.0",
    "concurrently": "^7.0.0",
    "jest": "^27.5.1",
    "jest-circus": "^27.5.1",
    "jest-html-reporters": "^3.0.5",
    "supervisor": "^0.12.0",
    "ts-jest": "^27.1.3"
  }
}
