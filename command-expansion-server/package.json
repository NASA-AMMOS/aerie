{
  "name": "aerie-command",
  "version": "1.0.0",
  "description": "command service for AERIE",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "copy-preface": "cp ./src/lib/codegen/CommandEDSLPreface.ts ./build/lib/codegen/CommandEDSLPreface.ts;",
    "reformat": "npx prettier --config ./.prettierrc -w ./src",
    "build": "tsc -p tsconfig.build.json; npm run copy-preface;",
    "start": "node --experimental-vm-modules ./build/app.js",
    "ts-watch": "npm run copy-preface; tsc -p tsconfig.build.json --watch",
    "dev-server": "supervisor -- --inspect=0.0.0.0 --experimental-vm-modules ./build/app.js",
    "dev": "npm run build && concurrently -n \"tsc,node\" -c \"blue,red\" \"npm run ts-watch\" \"npm run dev-server\"",
    "debug": "node --inspect=0.0.0.0:9229 --experimental-vm-modules ./build/app.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@js-temporal/polyfill": "^0.4.0",
    "@nasa-jpl/aerie-ampcs": "^1.0.0",
    "@nasa-jpl/aerie-ts-user-code-runner": "^0.3.2",
    "body-parser": "^1.19.2",
    "dataloader": "^2.0.0",
    "express": "^5.0.0-beta.1",
    "graphql-request": "^4.0.0",
    "pg": "^8.7.3",
    "piscina": "^3.2.0",
    "postgres-interval": "^4.0.0",
    "reserved-words": "^0.1.2",
    "typescript": "^4.5.5",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@microsoft/tsdoc": "^0.13.2",
    "@tsconfig/node16-strictest-esm": "^1.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.21",
    "@types/pg": "^8.6.4",
    "@types/reserved-words": "^0.1.0",
    "concurrently": "^7.0.0",
    "dotenv": "^16.0.0",
    "form-data": "^4.0.0",
    "jest": "^27.5.1",
    "jest-circus": "^27.5.1",
    "jest-extended": "^2.0.0",
    "jest-html-reporters": "^3.0.5",
    "node-fetch": "^3.2.10",
    "prettier": "^2.6.2",
    "supervisor": "^0.12.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0"
  }
}
